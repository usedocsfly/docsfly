{
  "name": "docsfly",
  "version": "1.0.0",
  "description": "Docsfly documentation framework monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "!packages/create-docsfly-app/templates/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "turbo": "^2.5.4"
  },
  "packageManager": "bun@1.0.0"
}